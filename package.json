{
  "name": "@eheikes/project-updater",
  "version": "0.5.0",
  "description": "Tool to automate project maintenance",
  "main": "update-project.js",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "update-project": "./update-project.js"
  },
  "scripts": {
    "test": "NODE_ENV=test JASMINE_CONFIG_PATH=./test/jasmine.json nyc jasmine",
    "lint": "standard --fix --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eheikes/project-updater.git"
  },
  "author": "Eric Heikes <eheikes@gmail.com>",
  "license": "Apache-2.0",
  "keywords": [
    "automation",
    "maintenance",
    "project",
    "tool",
    "update"
  ],
  "bugs": {
    "url": "https://github.com/eheikes/project-updater/issues"
  },
  "homepage": "https://github.com/eheikes/project-updater#readme",
  "dependencies": {
    "colors": "^1.2.1",
    "dot-prop": "^4.2.0",
    "execa": "^0.9.0",
    "extend": "^3.0.1",
    "fs-extra": "^5.0.0",
    "js-yaml": "^3.10.0",
    "listr": "^0.13.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "parse-packagejson-name": "^1.0.1",
    "pify": "^3.0.0",
    "spdx-license-list": "^4.0.0"
  },
  "devDependencies": {
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nyc": "^11.4.1",
    "proxyquire": "^2.0.0",
    "standard": "^11.0.0",
    "tempy": "^0.2.1"
  },
  "contributors": [
    "Eric Heikes <eheikes@gmail.com>"
  ],
  "files": [
    "lib",
    "tasks",
    "templates",
    "update-project.js"
  ],
  "standard": {
    "env": {
      "jasmine": true,
      "node": true
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "include": [
      "lib/*.js",
      "tasks/*.js",
      "*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": false,
    "all": false,
    "report-dir": "./coverage"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
